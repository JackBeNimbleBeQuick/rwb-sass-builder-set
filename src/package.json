{
  "name": "iosWebview",
  "version": "0.0.1",
  "description": "unified component styles",
  "main": "public/index.html",
  "dependencies": {
    "node-sass": "^4.12.0",
    "serve": "^11.0.0"
  },
  "devDependencies": {},
  "watch": {
    "css" : {
      "patterns" : ["sass"],
      "extensions": "scss"
    }
  },
  "scripts": {
    "setup": "bash setup.sh",
    "install": "bash globals.sh && npm run setup",
    "css": "node-sass ./sass/main.scss -o ../public/css",
    "server": "serve -l tcp://localhost:8999 ../public/ &",
    "sassing": "npm-watch css",
    "start": "npm run server && npm-watch css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JackBeNimbleBeQuick/responsiveSASS.git"
  },
  "keywords": [
    "SASS Quick Kick Start"
  ],
  "author": "Jack Harrison",
  "license": "CC-BY-4.0",
  "bugs": {
    "url": "https://github.com/JackBeNimbleBeQuick/responsiveSASS/issues"
  },
  "homepage": "https://github.com/JackBeNimbleBeQuick/tsResponsivePackKickstart#readme"
}
